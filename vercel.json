{
  "version": 2,
  "builds": [
    {
      "src": "api.py",
      "use": "@vercel/python"
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "api.py"
    }
  ],
  "env": {
    "PYTHON_VERSION": "3.11",
    "PLAYWRIGHT_BROWSERS_PATH": "/tmp/ms-playwright"
  },
  "functions": {
    "api.py": {
      "maxDuration": 60,
      "memory": 3008
    }
  }
}
